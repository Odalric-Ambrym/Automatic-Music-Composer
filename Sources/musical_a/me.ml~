(* Musical Elements *)
open Ma;;

(* Gammes *)
let cMajeur = new gamme C 0 0 0 0 0 0;;
let dMajeur  = new gamme D 0 0 0 0 0 0;;
let dDorien  = new gamme D (-1) 0 (-1) 0 0 0;;
let cDorien  = new gamme C (-1) 0 (-1) 0 0 0;;
let fMajeur  = new gamme F 0 0 0 0 0 0;;


cMajeur#signature;;
cMajeur#norm;;
dDorien#signature;;
dDorien#norm;;
cMajeur#expand;;
dDorien#expand;;
dMajeur#expand;;
fMajeur#expand;;


